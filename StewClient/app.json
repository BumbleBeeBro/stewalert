{
  "expo": {
    "name": "Stew Alert",
    "slug": "StewClient",
    "privacy": "public",
	"sdkVersion": "35.0.0",
	"android": {
	  "package": "com.bumblebeebro.stewalert",
	  "googleServicesFile": "./resources/google-services.json",
    },
    "platforms": [
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}